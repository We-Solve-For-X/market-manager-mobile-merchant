{
  "expo": {
    "name": "Market Manager",
    "description": "Merchant-application for the Market Manager system.",
    "slug": "marketmanager-irene-merchant-mobile",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "primaryColor": "#404A58",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/**"
    ],
    "ios": {
      "bundleIdentifier": "com.solveforx.marketmanager.merchant",
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.solveforx.marketmanager.merchant",
      "versionCode": 1
    }
  }
}